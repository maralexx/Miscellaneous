   "source": [
    "from sys import stdout\n",
    "\n",
    "l, r = 1, 1000000\n",
    "while l!=r:\n",
    "    mid = (l + r + 1) // 2\n",
    "    print(\"%d\\n\" % mid)\n",
    "    stdout.flush()\n",
    "    \n",
    "    response = input()\n",
    "    if response=='<':\n",
    "        r = mid - 1\n",
    "    else:\n",
    "        l = mid\n",
    "    \n",
    "print(\"! %d\\n\" % l)\n",
    "stdout.flush()"
   ]
  }
